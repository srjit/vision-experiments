
function E,sigma = estimate_noise(ImageArray)

    height = 5;
    width = 5;

    E = uint8(zeros(height,width));

    for k = 1:3
        for i = 1:width
            for j = 1:height
                E(i,j) = E(i,j) + ImageArray(k).Image(i,j);
            end
        end
    end

    E = 1/(width * height) * E;
end


    